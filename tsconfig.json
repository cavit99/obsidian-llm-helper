{
  "compilerOptions": {
    "target": "ES2018",
    "module": "Node16",
    "moduleResolution": "Node16",
    "lib": ["ES2018", "DOM"],
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["node"]
  },
  "include": ["src/**/*.ts"]
}
